<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.dev.alleon.mappers.welfare.HouseholdTypeMapper">
    <select id="selectAll" resultType="com.dev.alleon.entities.CodeEntity">
        SELECT `code` AS `code`,
               `display_text` AS `displayText`
        FROM `aeo`.`household_type`
    </select>
</mapper>